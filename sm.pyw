    def bonus_months_null(self):
        get_value = self.bonus_months_number.value()
        if get_value ==0:
            self.bonus_value.setDisabled(True)
        else:
            self.bonus_value.setDisabled(False)

    def convert_to_float(self, number):
        return float(number)

    def convert_to_int(self, number):
        return int(number)

    def currency_format(self, amount, currency):
        if currency == "US Dollar":
            currency = "${:,.2f}".format(amount)
        elif currency == "Euro":
            currency = "â‚¬{:,.2f}".format(amount)
        else:
            currency = "{:,.2f} DH".format(amount)
        return currency

    def get_inputs(self):

        # collect Information
        currency_type = self.currency_type.currentText()
        monthly_salary = self.convert_to_float(self.monthly_salary.text())
        monthly_charges = self.convert_to_float(self.monthly_charges.text())
        bonus_months_number = self.convert_to_int(self.bonus_months_number.text())
        bonus_value = self.convert_to_float(self.bonus_value.text())
        years_number = self.convert_to_int(self.years_number.text())

        # process
        if bonus_months_number == 0:
            bonus_value = 0
            self.bonus_value.setDisabled(True)

        year_salary = monthly_salary * (12 - bonus_months_number) + bonus_months_number * bonus_value
        year_charges = monthly_charges * 12
        year_net_profit = year_salary - year_charges
        x_year_net_profit = year_net_profit * years_number
        x_year_salary = year_salary * years_number
        x_year_charges = year_charges * years_number
        needs = monthly_salary * 0.5
        wants = monthly_salary * 0.3
        saving = monthly_salary * 0.2

        now = datetime.now()
        today_year = now.year

        # output
        self.label_7.setText("Annual (Years Statistics :: " + str(now.day)+'/'+str(now.month)+'/'+str(today_year+years_number)+")")
        self.x_year_salary_label_text.setText("Salary in "+str(years_number)+" years:")
        self.x_year_salary_label_value.setText(self.currency_format(x_year_salary,currency_type))

        self.x_year_charges_label_text.setText("Charges in "+str(years_number)+" years:")
        self.x_year_charges_label_value.setText(self.currency_format(x_year_charges,currency_type))

        self.x_year_net_profit_label_text.setText("Net Profit in "+str(years_number)+" years:")
        self.x_year_net_profit_label_value.setText(self.currency_format(x_year_net_profit,currency_type))


        self.needs_label_text.setText("Max Needs per month:")
        self.needs_label_value.setText(self.currency_format(needs,currency_type))
        self.wants_label_text.setText("Max Wants per month:")
        self.wants_label_value.setText(self.currency_format(wants,currency_type))
        self.saving_label_text.setText("Min Saving per month:")
        self.saving_label_text_2.setText(self.currency_format(saving,currency_type))



